<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Proxytype</Authors>
    <Version>0.0.15</Version>
    <Company>RudeNet</Company>
    <Description>Hide external files inside jpg image, most jpg encoders looks only the image stream and ignore other data.</Description>
    <AssemblyVersion>0.0.1.5</AssemblyVersion>
    <FileVersion>0.0.1.5</FileVersion>
    <PackageReleaseNotes>RaperCommon - common stuff.
RaperWriter - write and merge file to image
RaperReader - export hidden file from image</PackageReleaseNotes>
    <PackageTags>Files, Images, Archives</PackageTags>
    <RepositoryType>Github</RepositoryType>
    <RepositoryUrl>https://github.com/proxytype/JpgRaper</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>